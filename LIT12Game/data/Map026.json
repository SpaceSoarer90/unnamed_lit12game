{"id":5,"name":"ParentFight","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":101,"indent":0,"parameters":["",0,0,2,"Alfonso"]},{"code":401,"indent":0,"parameters":["Well, look who finally decided to come home. "]},{"code":401,"indent":0,"parameters":["And where exactly have you been, young lady?"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Miriam"]},{"code":401,"indent":0,"parameters":["Who was that boy you were with at the park?"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Alfonso"]},{"code":401,"indent":0,"parameters":["Do you know how bad that looks? "]},{"code":401,"indent":0,"parameters":["A teenage girl like you, "]},{"code":401,"indent":0,"parameters":["running around with some random boy!"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Maria"]},{"code":401,"indent":0,"parameters":["What? What are you talking about? He’s my friend. "]},{"code":401,"indent":0,"parameters":["We were just playing because it’s my birthday…"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Miriam"]},{"code":401,"indent":0,"parameters":["Oh, so now you’re playing the innocent one? "]},{"code":401,"indent":0,"parameters":["Don’t lie to us, Maria! "]},{"code":401,"indent":0,"parameters":["Do you think this is how we raised you? Sneaking around with boys? "]},{"code":401,"indent":0,"parameters":["And what’s with that flower? Did he give it to you"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Maria"]},{"code":401,"indent":0,"parameters":["He didn’t give me anything! I picked it myself!"]},{"code":401,"indent":0,"parameters":["Why are you jumping to conclusions?"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Alfonso"]},{"code":401,"indent":0,"parameters":["Don’t use that tone with your mother, Maria. "]},{"code":401,"indent":0,"parameters":["Respect your parents! We’re trying to protect you!"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Maria"]},{"code":401,"indent":0,"parameters":["Protect me? From what? "]},{"code":401,"indent":0,"parameters":["I can’t even have one friend "]},{"code":401,"indent":0,"parameters":["without you assuming the worst"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Miriam"]},{"code":401,"indent":0,"parameters":["You don’t get to talk to us like this, Maria! You’re just a child. "]},{"code":401,"indent":0,"parameters":["You don’t understand how the world works!"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Maria"]},{"code":401,"indent":0,"parameters":["Can’t you see how unfair you’re being? "]},{"code":401,"indent":0,"parameters":["I just wanted to spend time with "]},{"code":401,"indent":0,"parameters":["someone who doesn’t treat me like a burden!"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Alfonso"]},{"code":401,"indent":0,"parameters":["That’s enough! Go to your room "]},{"code":401,"indent":0,"parameters":["if you can’t control yourself!"]},{"code":101,"indent":0,"parameters":["",0,0,2,"Maria"]},{"code":401,"indent":0,"parameters":["Why is it so hard for you to believe me?"]},{"code":401,"indent":0,"parameters":["I didn’t do anything wrong!"]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":16,"y":5},
